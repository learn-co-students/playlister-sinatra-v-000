<h1>New Song</h1>
<form method="POST" action="/songs">
  <input type="text" id="Name" name="name" placeholder="Name">
  <input type="text" id="Artist Name" name="artist" placeholder="Artist">
  <% Genre.all.each do |genre| %>
  <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%= genre.name %></input>
  <% end %>
  <input type="submit" value="Create">
</form>
