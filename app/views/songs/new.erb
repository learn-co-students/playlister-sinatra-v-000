<form class="" action="/songs/new" method="post">
  <p>
    Name: <input type="text" name="name" value="" id="Name">
  </p>
  <p>
    Genre:<br><br>
    <%Genre.all.each do |genre|%>
    <input type="checkbox" name="genres[]" value="<%=genre.id%>" id="<%=genre.name%>"><%=genre.name%></input>
    <%end%>
  </p>
  <p>
    <label>Artist Name:</label><input type="text" name="artist_name" value="" id="Artist Name">
  </p>
  <input type="submit"  value="Create">

</form>
