<form action="/songs" method="POST">
  Song Name: <input type="text" name="song[name]" id="Name"><br><br>
  Artist Name: <input type="text" name="artist[name]" id="Artist Name"><br><br>

  Genres:<br>
  <% Genre.all.each do |genre| %>
     <input id="<%=genre.name%>" type="checkbox" name="song[genre_ids][]" value="<%=genre.id%>"><%= genre.name %>
  <% end %>
  <br><br>
  <input type="submit" value="Create">
</form>
