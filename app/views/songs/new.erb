<form action="/songs" method="post">
  <label>Song Name:</label>
  <input type="text" name="song[name]" id="Name">

  <br></br>

  <label>Artist:</label>
  <input type="text" name="artist[name]" id="Artist Name">

  <br></br>

  <label>Genres:</label>
  <% Genre.all.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%= genre.name %></input>
  <% end %>

  <br></br>

  <input type="submit" value="Create">
</form>
