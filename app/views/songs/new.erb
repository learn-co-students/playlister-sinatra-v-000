<h1>Create a New Song</h1>
<form action="/songs" method="/post">
  <p>Name <input type="text" name="song_name" id="Name"></p>
  <p>Artist Name: <input type="text" name="artist_name" id="Artist Name"></p>
  <p>Genres: <% Genre.all.each do |genre| %>
              <input type="checkbox" id="<%= genre.name %>" name="genres[]" value="<%= genre.id %>">
              <%= genre.name %></input>
  <% end %>
</p>
  <input type="submit" value="Create">
</form>