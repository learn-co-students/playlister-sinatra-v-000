<form action="/songs" method="post">
  <label for="">Song Name: </label><br>
  <input type="text" name="Name" value=""><br>
  <label for="">Choose Genre: </label><br>
  <% Genre.all.each do |genre| %>
    <input type="checkbox" name="[genres][]" id="<%= genre.name %>" value="<%= genre.id %>"><%= genre.name %><br>
  <% end %>
  <label for="">Artist Name: </label><br>
  <input type="text" name="Artist Name" value="">
  <input type="submit" name="" value="Create">
</form>
