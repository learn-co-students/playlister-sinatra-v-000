<form method="post" action="/songs">
	<label>Artist Name:</label> <input type='text' name='song[artist][name]' id='Artist Name'>
	<label>Song Name:</label> <input type='text' name='song[name]' id='Name'>
    <label>Genres:</label>
    <%@genres.each do |genre|%>
    <p><input id="<%=genre.name%>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%=genre.name%></p>
    <%end%>
    <input type="submit" value="Create" id="Create">
</form>