<form action="/songs" method="POST">
  <label for="Name">Name:</label>
  <br><br>
  <input type="text" name="song[name]" id="Name">
  
  <br><br>

  <label>Genre(s):</label>
  <br><br>
  <% Genre.all.each do |genre| %>
    <input type="checkbox" name="genres[]" value="<%= genre.id %>" id="<%= genre.name %>">
    <%= genre.name %></input>
  <% end %>
<br><br>

  <label>Artist:</label>
<br><br>
  <input type="text" name="artist[name]" id="Artist Name">

  <input type="submit" value="Create Song" id="Create">


</form>