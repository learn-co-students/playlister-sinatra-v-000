<form method = 'POST' action = '/songs'>
  Song Name: <input type = 'text' name = 'song_name' id = "Name"><br>
  Artist Name: <input type = 'text' name = 'artist_name' id = "Artist Name"><br>
  Choose the applicable genres:<br>
  <% Genre.all.each do |genre|  %>
  <input type="checkbox" id="<%= genre.name %>" name="genres[]" value="<%= genre.id %>"><%= genre.name %> <br>
  <% end %>
  <input type = 'submit' name = "Create" id = "Create" value = "Create">
</form>
