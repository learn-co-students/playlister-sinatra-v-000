<h1>Add new Song</h1>
<form action="/songs" method="POST">
  <br><label>Artist Name: </label>
  <input type="text" name="Artist Name"></br>
  <br><label>Song Name: </label>
 <input type="text" name="Name"></br>

 <br><label>Genres</label></br>
<% Genre.all.each do |genre| %>
  <input type="checkbox" name="genres[]" value="<%= genre.id %>" id="<%= genre.name %>"><%= genre.name %></input>
<% end %>

 <input type="submit" value="Create">

</form>