<h1>Create a new Song</h1>
<form class="" action="/songs" method="post">
   <label>Name:</label>
   <input type="text" name="Name" id="song_name"><br><br>
   <label>Genre:</label>
   <% Genre.all.each do |genre| %>
   <input type="checkbox" name="genres[]" value="<%= genre.id %>" id="<%= genre.name %>"><%= genre.name %></input>
   <% end %><br><br>
   <label>Artist:</label>
   <input type="text" name="Artist Name" id="Artist Name"><br><br>
   <input type="submit" name="Create" value="Create">
</form>
