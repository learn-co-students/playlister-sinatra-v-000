
<form action="/songs" method="POST">
  Song Name: <input type="text" name="song[name]" id="Name"/><br></br>
  Genres:<br></br>
  <%@genres.each do |genre|%>
    <input type="checkbox" name="song[genre_ids][]" value="<%=genre.id%> "id="<%=genre.name%>"><%=genre.name%></input>
    <br></br>
  <%end%>
  Artist: <input type="text" name="artist[name]" id="Artist Name"/><br></br>

  <input type="submit" value="Create">
</form>
