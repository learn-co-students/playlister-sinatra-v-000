<form method="post" action="/songs">
  <p>Artist: <input type="text" name="Artist Name"></p>

  <p>Song Name: <input type="text" name="Name"></p>

  <p>Genre:</p>
  <% Genre.all.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%=genre.name%></br>
  <% end %>

  <input type="submit" value="Create">
</form>
