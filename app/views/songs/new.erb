<h1>Add A New Song</h1>
<form action="/songs" method="POST">
  <p><label>Song Name: </label><input type="text" name="song_name" id="Name"></p>
  
  <p><label>Artist Name: </label><input type="text" name="artist_name" id="Artist Name"></p>
  
  <p><label>Genres: </label></p>
  <% Genre.all.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox"  name="genres[]" value="<%= genre.id %>"><%= genre.name %>
  <% end %>
  
  <p><input type="submit" id="Create" value="Create"></p>
</form>