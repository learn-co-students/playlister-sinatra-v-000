<h1>Create a new song:</h1>

<form action="/songs" method="POST">
  <label>Name</label>
  <input type="text" name="Name" id="Name"/>

  <br></br>

  <%Genre.all.each do |genre|%>
    <input type="checkbox" name="<%=genre.name%>" value="<%=genre.name%>" id="<%=genre.name%>"><%=genre.name%></input>
  <%end%>

  <br></br>
  <label>Artist Name</label>
  <input type="text" name="artist" id="Artist Name"/>

  <br></br>
  <input type="submit" value="Create">
</form>
