<form method="post" action="/songs">

<label>Name</label>
<input type="text" id="Name" name="song[name]">
<label>Artist Name</label>
<input type="text" id="Artist Name" name="song[artist]">

<label>Song Genre:</label>
  <% Genre.all.each do |genre| %>
  <input id="<%= genre.name %>" type="checkbox" name="song[genre_ids][]" value="<%= genre.id %>"><%= genre.name%>
  <% end %>
<input id="Create" type="submit" name="submit">
</form>