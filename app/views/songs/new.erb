<h1>Create New Song</h1>

<br><br>

<form method="POST" action="/songs">
  <label>Artist Name:<input type="text" name="Artist Name"></label><br>
  <label>Song Name:<input type="text" name="Name"></label>

  <br><br>


  <label>Genres:</label><br>
  <%Genre.all.each do |genre|%>
    <%=genre.name%> <input id="<%=genre.name%>" type="checkbox" name="genres[]" value="<%=genre.id%>"><br>
  <%end%>
  <br>
  <input type="submit" value="Create">
</form>
