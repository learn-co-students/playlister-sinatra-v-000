<form class="" action="/songs" method="post">
  <label for="name">Name</label>
  <input type="text" name="Name" value="" id="name">

  <br><br>

  <% @artists.all.each do |artist| %>
    <input type="checkbox" name="song[artist_id]" value="<%= artist.id %>">
      <%= artist.name %>
    </input>
  <% end %>

  <br><br>

  <% Genre.all.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>">
  <% end %>

  <br><br>

  <label for="">Artist Name</label>
  <input type="text" name="Artist Name" value="">

  <br><br>

  <input type="submit" name="Create" value="Create">
</form>
