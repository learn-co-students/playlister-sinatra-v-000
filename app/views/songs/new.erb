<h1>Create Song</h1>
<form action="/songs/new" method="post">

  <label>Name:</label>
  <input type="text" name="Name">

  <% Genre.all.each do |g| %>
    <input type="checkbox" name="genre[name]" value="<%=g.name%>" id="<%=g.name%>">
  <% end %>

  <label>Genre Name:</label>
  <input type="text" name="Genre Name">

  <label>Artist Name:</label>
  <input type="text" name="Artist Name">

  <input type="submit" value="Create">

</form>
