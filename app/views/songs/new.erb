<div class="jumbotron">
	<h2>Create New Song</h2>
</div>
<div class="container">
	<form action="/songs" method="post">
		<div class="form-group">
		<label for="">Artist Name:<input class="form-control" type="text" name="artist_name"></label>
		</div>
		<div class="form-group">
		<label for="">Name:<input class="form-control" type="text"  id="Name" name="song_name"></label>
		</div>
		<div class="form-check">
		<label class="form-check-label">Genre:
		<br>
		 <%  Genre.all.each do |genre|%>
			<input class="form-check-input" type="checkbox" id="<%=genre.name%>" name="song[genre][]" value="<%=genre.name%>"> <%=genre.name%></input>
		<% end %>
		</label>
		</div>
		<br>
		 <button type="submit" class="btn btn-primary">Create New Song</button>
	</form>
</div>