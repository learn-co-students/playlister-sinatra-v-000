<h1>Add A Song</h1>
<form action="/songs" method="POST">
<p><label for="Name">Song Name: </label>
<input type="text" name=song[name] id="Name"><p>
<p>Genre:</p>
<p><% @genres.each do |genre| %>
    <input type="checkbox" name="song[genre_ids][]" value="<%=genre.id%>" id="<%=genre.name%>"><%= genre.name %></input>
<% end %></p>
<p><label for="Artist Name">Artist Name: </label>
<input type="text" name="song[artist]" id="Artist Name"></input></p>
<input type="submit" value="Create">
</form>