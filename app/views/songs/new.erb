<h1>Create a New Song</h1>

<br>

<form method="POST" action="/songs">
  <label>Name: </label>
  <input type="text" name="Name"><br></br>
  <label>Artist: </label>
  <input type="text" name="Artist Name"><br></br>
  <% @genres.each do |genre| %>
    <input type="checkbox" name="genres[]" value="<%= genre.id %>" id="<%= genre.name %>"><%= genre.name%></input>
    <% end %><br></br>
    <input type="submit" value="Create">
</form>
