<form action="/songs" method="POST" >
     <label>Name:</label>
     <input type="text" name="song[name]" id="song_name">
     
     <label>Songs:</label>
    <br>
     <input type="text" name="song[name]" id="new_song">
     <%@songs.each do |song|%>
     <label><%=song.name%></label>
     <input type="checkbox" name="artist[song_ids][]" id="song_<%=song.id%>" value="<%=song.id%>">
     <%end%>
   
     <input type="submit" value="Create New Song">
</form>