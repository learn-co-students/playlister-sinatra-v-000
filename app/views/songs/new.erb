<h1>Create a Song</h1>
<form action="/songs" method="post">
  <label for="Name">Name:</label>
  <input type="text" name="song[name]" id="Name">
  <br><br>
  <label for="Artist Name">Artist:</label>
  <input type="text" name="artist[name]" id="Artist Name">
  <br><br>

  <label for="genre">Choose 1 or more genres</label>
  <%Genre.all.each do |genre|%>
    <input type="checkbox" name="song[genre_ids][]" value="<%=genre.id%>" id="<%=genre.name%>"><%=genre.name%></input>
  <%end%>
  <br><br>
  <input type="submit" value="Create Song" id="Create">
</form>