<h1>Enter New Song</h1>

<form class="" action="/songs/new" method="post">
  <label for="">Name:</label>
  <input type="text" name="song[name]" id="Name" value="">

  <br></br>

  <label for="">Artist:</label>
  <input type="text" name="song[artist][name]" id="Artist Name" value="">

  <br></br>

  <label for="">Select Genre:</label>
  <%Genre.all.each do |obj|%>
  <input type="checkbox" name="song[genre][][name]" id="<%=obj.name%>" value="<%=obj.name%>"><%=obj.name%></input>
  <%end%>

  <br></br>

  <input type="submit" name="" value="Create Song">

</form>
