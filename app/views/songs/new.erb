<h1>Create a Song</h1>
<form method="POST" action="/songs">
  <label for="song_name">Song Name:</label>
  <input type="text" name="song_name" id="Name">
  <br><br>

  <label for="artist_name">Artist Name:</label>
  <input type="text" name="artist_name" id="Artist Name">

  <br><br>

  <h2>Pick an existing genre/genres</h2>
    <%Genre.all.each do |genre|%>
      <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%= genre.name %><br></input>
    <%end%>


  <input type="submit" value="Create">



</form>
