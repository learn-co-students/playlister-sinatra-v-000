<h2>Create New Song</h2>
<form action='/songs' method='post'>

  <label>Song Name:</label>
  <input type="text" name="song_name" id="Name">
  <br></br>

  <label>Artist:</label>
  <input type="text" name="artist_name" id="Artist Name">
  <br></br>

  <label>Genre:</label>
  <br></br>
  <% Genre.all.each do |genre| %>
    <input type="checkbox" name="genres[]" value="<%=genre.id%>" id="<%=genre.name%>"><%=genre.name%></input>
  <% end %>

  <br></br>
  <input type="submit" value="Create">

</form>
