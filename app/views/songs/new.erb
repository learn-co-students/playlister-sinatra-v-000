<form method='POST' action='/songs'>
    Song Name: <input type='text' name='song[name]' id='Name'>
    Artist Name: <input type='text' name='song[artist][name]' id='Artist Name'>
    Genres:
    <%@genres.each do |genre|%>
    <p><input id="<%=genre.name%>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%=genre.name%></p>
    <%end%>
    <input type="submit" value="Create" id="Create">
</form>
