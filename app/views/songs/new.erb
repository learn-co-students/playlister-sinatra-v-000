<h1>Create a Song</h1>
<form  action="/songs" method="post">
  <label>Name:</label>
  <input type="text" name="song[name]">
  <br></br>
  <label>Artist:</label>
  <input type="text" name="artist[name]">
  <br></br>
  <label>Genre(s):</label>
  <%Genre.all.each do |g|%>
    <input type="checkbox" name="genres[]" value="<%=g.id%>"><%=g.name%></input>
  <%end%>
  <br></br>
  <input type="submit" value="Create">

</form>
