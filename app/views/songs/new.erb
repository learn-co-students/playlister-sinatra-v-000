<h1> New Song</h1>

<form action="/songs" method="post">

  Song Name: <input type="text" name=song[name] id="Name"> </input>
  <br>
  <% Genre.all.each do |genre| %>
      Genres: <input type="checkbox" name="song[genre_ids][]"  value="<%=genre.id%>" id="<%=genre.name%>"> <%=genre.name%> </input>\
  <% end %>

  Artist Name<input type="text" name="song[artist]" id="Artist Name"> </input>


  <br>
  <input type="submit" id="Create" name ="Create">
</form>