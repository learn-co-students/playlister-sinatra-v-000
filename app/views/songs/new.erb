<form action="/songs" method="POST">
  <label>Name:</label>
  <input name="song[name]" type="text" id="Name">
  <br></br>
  <label>Artist Name:</label>
  <input name="artist" type="text" id="Artist Name">
  <br></br>
  <label>Genres:</label>
  <%Genre.all.each do |g|%>
  <%#binding.pry%>
  <input name="genres[]" type="checkbox" id="<%=g.name%>" value="<%=g.name%>"><%=g.name%></input>
  <%end%>
  <br></br>

    <input type="submit" value="Create" id="Create">
</form>
