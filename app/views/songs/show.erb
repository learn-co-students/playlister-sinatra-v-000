<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
<h1>Song Name: <%=@song.name%></h1>

<h3> Artist: </h2>
<label>
<a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a> </label>
<h3> Genres </h3>
<label>
<% @song.genres.each do |genre| %>
<a href="/genres/<%=genre.slug%>"><%= genre.name %></a>
</label>
<% end %>