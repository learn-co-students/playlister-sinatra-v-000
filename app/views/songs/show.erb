<h1>Song Name: <%=@song.name%></h1>

<br><br>
<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<!--Displays Songs Artist list-->
<h2>Artist:</h2>
  <label>
    <a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a>
  </label><br>

<br><br>

<!--Displays Songs Genres List-->
<h2>Genres:</h2>
<%@song_genres.each do |genre|%>
  <label>
    <a href="/genres/<%=genre.slug%>"><%=genre.name%></a>
  </label><br>
<%end%>
