<h1><%= @song.name %></h1>
<h3><a href="/artists/<%= @artist.slug %>"><%= @artist.name %></a></h3>
<% @genres.each do |genre| %>
  <h3><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></h3>
<% end %>
<form action="/songs/<%= @song.slug %>/delete" method="post">
  <input type="hidden" name="_method" value="delete" id="hidden">
  <input type="submit" value="delete">
</form>
