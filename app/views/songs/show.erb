<% unless locals.empty? %>
  <%= message %>
<% end %>
<%=@song.name%>
<h1>Song Name: <%=@song.name %></h1>
<h1>Artist: <a href="/artists/<%=@song.artist.slug%>"><%= @song.artist.name %></a></h1>
<%@song.genres.each do |genre|%>
<h3>Genre(s): <a href="/genres/<%=genre.slug%>"><%= genre.name %></a></h3>
<% end %>
