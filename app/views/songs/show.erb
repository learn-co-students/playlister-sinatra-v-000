<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
<h1>Song Title: <%= @song.name %></h1>
<h3>Artist:<a href="/artists/<%=@song.artist.slug%>"> <%= @song.artist.name %> </a></h3>
<h3>Genres: </h3>
<% @song.genres.each do |genre|%>
<input type="checkbox" name="<%=genre.name%>" id="genre">
<h5>Genre:<a href="/genres/<%=genre.slug%>"> <%= genre.name %> </a></h5>
<%end%>
