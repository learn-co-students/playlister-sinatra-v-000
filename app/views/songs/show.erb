<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1><%=@song.name%></h1>

<h3>Artist:</h3>
  <li>
    <%unless @song.artist.nil?%>
      <a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a><br>
    <%end%>
  </li>

<h3>Genres:</h3>
  <li>
    <%@song.genres.each do |object|%>
       <a href="/genres/<%=object.slug%>"><%=object.name%><br>
    <%end%>
  </li>
