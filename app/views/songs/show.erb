<h1>Song: <%= @song.name %></h1>
<ul>
  <li>
    <h3>Artist: </h3>
    <ul>
      <li>
        <a href="/artists/<%= @artist.slug %>"><%= @artist.name %></a>
      </li>
    </ul>
  </li>
  <li>
    <h3>Genres: </h3>
    <ul>
      <% @genres.each do |genre| %>
        <li>
          <a href="/genres/<%= genre.slug %>"><%= genre.name %></a>
        </li>
        
      <% end %>
    </ul>
  </li>
</ul>
