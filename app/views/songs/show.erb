<% unless locals.empty? %>
<%= message %>
<% end %>


<h1>Song Name: <%=@song.name%></h1>

<h2>Song's Artist:<a href="/artists/<%= @song.artist.slug %>"> <%= @song.artist.name %> </a></h2>

<h2>Song's Genres</h2>
<% @song.genres.each do |genre| %>
  <p><a href="/genres/<%= genre.slug %>"><%= genre.name %> </a></p>
<% end %>