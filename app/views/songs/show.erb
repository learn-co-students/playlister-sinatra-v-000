<h3>Song: <%=@song.name%></h3>

<h4>Artist: <a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a></h4>

<h4>Genres:</h4>
<%@song.genres.uniq.each do |genre|%>
  <p><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></p>
<%end%>

<%if flash.has?(:message)%>
  <p><%=flash[:message]%></p>
<%end%>
