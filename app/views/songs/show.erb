<% if flash.has?(:message) %>
  <h1><%=flash[:message]%></h1>
<%end%>
<h2><%= @song.name %></h2>


<h3><a href="/artists/<%=@song.artist.slug%>"><%= @song.artist.name %></a></h3>

<% @song.genres.each do |genre| %>
  <li>
    <a href="/genres/<%=genre.slug%>"><%= genre.name %></a>
  </li>
<% end %>

<br></br>
<h4><a href="/songs/<%=@song.slug%>/edit">edit song</a></h4>
