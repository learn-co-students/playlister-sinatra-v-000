<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1><%= "#{@song.name}" %></h1>

<h2> Artist: <a href="/artists/<%= @song.artist.slug%>"> <%= @song.artist.name %> </a></h2>

<h3>Genres</h3>

<ul>
<% @song.genres.each do |s| %>
<a href="/genres/<%= s.slug %>"><%= s.name %></a></li>
<% end %>
</ul>
