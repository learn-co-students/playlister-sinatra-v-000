<% if flash.has?(:notice) %>
  <%= flash[:notice] %>
<% end %>
<h3>"Successfully created song."</h3>
<h1><%= @song.name %></h1>

<h2><a href='/artists/<%= "#{@song.artist.id}" %>'><%= @song.artist.name %></a></h2>

<% @song.genres.each do |genre| %>
  <h2><a href='/genres/<%= "#{genre.id}" %>'><%= genre.name %></a></h2>
<% end %>