<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
<h1><%=@song.name%></h1>
<label>Artist: <a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a></label><br>
<%@song.genres.each do |genre|%>
<label>Genres: <a href="/genres/<%=genre.slug%>"><%=genre.name%></a></label>
<%end%>
