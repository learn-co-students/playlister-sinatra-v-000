<%if flash.has?(:message)%>
  <%=flash[:message]%>
<%end%>

<h1> Song Name: <%=@song.name%></h1>
<h2> Artist: <a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a></h2>
<h3> Genre(s):
  <%@song.genres.each do |genre| %>
  <a href='/genres/<%=genre.slug%>'><%= genre.name %></a>
  <%end%>
</h3>
