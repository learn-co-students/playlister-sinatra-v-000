
<h1>Song:</h1>

<h2><%=@song.name%></h2>
<%#binding.pry%>
<h3>Artist: <a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a></h3>
<%@song.genres.each do |g|%>
<%#binding.pry%>
<h3>Genre: <a href="/genres/<%=g.slug%>"><%=g.name%></a></h3>
<%end%>
<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
