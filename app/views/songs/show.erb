<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1><%=@song.name%></h1>

<p>Artist:
<a href="/artists/<%= @song.artist.slug %>"><%= @song.artist.name %></a></p>

<p>Genres:
  <%@song.genres.each do |genre|%>
  <ul><a href = "/genres/<%=genre.slug%>"><%=genre.name%></a></ul>
  <% end %>
</p>
