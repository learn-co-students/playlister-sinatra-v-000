<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
<h1><%= @song.name %></h1>
<br><a href="/artists/<%=@song.artist.slug%>"><%= @song.artist.name %></a></br>
<% @song.genres.each do |genre| %>
  <% if genre.name %>
      <br><a href="/genres/<%=genre.slug%>"><%= genre.name %></a></br>
  <%end %>
<% end%>
