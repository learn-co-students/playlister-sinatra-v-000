<% unless locals.empty? %>
  <%= message %>
<% end %>

<h1>Title: <%=@song.name%></h1>
<h1>Artist: <a href="/artists/<%=@song.artist.slug%>"><%= @song.artist.name %></a></h1>
<h1>Genre(s): <%@song.genres.each do |genre|%>
  <a href="/genres/<%=genre.slug %>"><%= genre.name %></a></h1>
  <%end%>

 <a href="/songs/<%=@song.slug%>/edit">Edit Song</a>