<% if flash.has?(:message) %> 
  <%= flash[:message] %>
<% end %>
<h1><%=@song.name%></h1>
<p> Artist: <a href="/artists/<%=@song.artist.slug%>"><%= @song.artist.name%></a> </p> 
<%@song.genres.each do |genre|%>
<p> Genre: <a href="/genres/<%=genre.slug%>"><%= genre.name%></a> </p> 
<%end%>

<a href="/songs/<%=@song.slug%>/edit">
    <button>Edit</button>
</a>