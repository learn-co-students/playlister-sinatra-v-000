<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1><%= @song.name %></h1>

<h2>Artist: <a href= <%= "/artists/#{@artist.slug}" %> > <%= @artist.name %></a> <br></h2>
<h2>Genre(s): 
<% @genres.each do |genre| %>
     <a href= <%= "/genres/#{genre.slug}" %> > <%= genre.name %></a>
<% end  %>
</h2>

