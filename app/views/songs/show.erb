<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
<ul>
<h4><label for="name">Song Name: </label><%= @song.name %></h4>
<li><%= @song.artist.name %></li>
<a href="/artists/<%= @song.artist.slug %>">Link to Artist</a>
<% @song.genres.each do |genre| %>
  <li> <%= genre.name %></li>
  <a href="/genres/<%= genre.slug %>">Link to Genre</a>
<% end %>
</ul>