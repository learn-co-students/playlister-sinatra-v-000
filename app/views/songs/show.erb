<% if flash.include?(:message) %>
<%= flash[:message] %>
<% end %>

<h1>Song Name: <%=@song.name%></h1>
<h2>Song Artist: <a href = "/artists/<%=@song.artist.slug%>"> <%=@song.artist.name%></a></h2>
<h2>Song Genres:
  <%@song.genres.each do |genre|%>
  <a href = "/genres/<%=genre.slug%>"><%=genre.name%></a></h2>
  <%end%>
