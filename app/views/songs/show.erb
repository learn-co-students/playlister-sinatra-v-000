Song Name: <a href="/songs/<%= @song.slug %>"><%= @song.name %></a><br/>
Artist Name: <a href="/artists/<%= @artist.slug %>"><%= @artist.name %></a><br/>
<%@song.genres.each do |genre| %>
  <a href="/genres/<%= genre.slug %>"><%= genre.name %></a><br/>
<% end %>

<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>