<% if flash.has?(:message) %>
   <%= flash[:message] %>
 <% end %>

 <h1>Song Name: <%= @song.name %> </h1>
 <br>
 <h2> Song Artist: <a href="/artists/<%= @song.artist.slug %>"><%= @song.artist.name %></a></h2>
 <h2>Songs Genres:</h2>
 <% @song.genres.each do |genre| %>
   <h3><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></h3>
 <% end %>

 <a href="/songs/<%= @song.slug %>/edit">Edit Song</a>
