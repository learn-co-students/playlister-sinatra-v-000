<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1>Song: <%= @song.name %></h1>
<h1>Artist: <a href='/artists/<%=@song.artist.slug%>'><%=@song.artist.name%></a></h1>
<h2>Genres: <%@song.genres.each do |genre| %>
  <a href='/genres/<%=genre.slug%>'><%=genre.name%></a>
  <%end%></h2>
