
<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1> <%= @song.name %> </h1>

Artist Name <a href="/artists/<%= @song.artist.slug %>"> <%= @song.artist.name %> </a>

Genre <% @song.genres.each do |genre| %>
        <a href="/genres/<%= genre.slug %>"><%= genre.name %> </a>
      <% end %>
