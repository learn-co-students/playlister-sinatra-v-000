<% if !!session[:create_message]  %>
  <%= session[:create_message] %>
<% end %>

<% if !!session[:update_message]  %>
  <%= session[:update_message] %>
<% end %>

<h1> <%= @song.name %> </h1>
<h3><a href="/songs">See Other Songs</a></h3>

Artist<a href="/artists/<%= @song.artist.slug %>"> <%= @song.artist.name %> </a>

Genres<% @song.genres.each do |genre| %>
  <a href="/genres/<%= genre.slug %>"><%= genre.name %> </a>
<% end %>
