<h1>Edit a song:</h1>

<form action="/songs" method="POST">
  <label>Name: <%=@song.name%></label>
  <br></br>

  <%Genre.all.each do |genre|%>
    <input type="checkbox" name="genre[]" value="<%=genre.id%>" id="<%=genre.name%>"><%=genre.name%></input>
  <%end%>

  <br></br>
  <label>Artist Name</label>
  <input type="text" name="artist" id="Artist Name"/>

  <br></br>
  <input type="submit" value="Save">
</form>
