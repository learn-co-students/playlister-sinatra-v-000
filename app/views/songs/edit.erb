<h1>Edit <%=@song.name%></h1>
<form action="/songs/:slug" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label>Song Title:</label>
  <input type="text" name="name" id="Name" value="<%=@song.name%>"
  <br></br>
  <label>Artist:</label>
  <input type="text" name="artist" id="Artist Name" value="<%=@song.artist.name%>">
</form>
