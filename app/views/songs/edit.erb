<h1>Songs Edit</h1>

<form class="" action="/songs/<%= @song.slug %>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  Name: <input type="text" name="song[name]" value="<%= @song.name %>"><br>
  Artist: <input type="text" name="artist[name]" value="<%= @song.artist.name %>"><br>
  Genres:
  <% Genre.all.each do |genre| %>
    <input type="checkbox" name="" value="<%= genre.name %>" id="<% genre.name %>"><br>
  <% end %>
  <br>
  <input type="submit" value="Create">

</form>
