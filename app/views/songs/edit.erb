<form method='post' action='/songs/<%= @song.slug%>'>
    <label for='artist[name]'>New Artist Name:</label>
    <input type='text' name='artist[name]' id="Artist Name"/>
    <input type='submit' value='Save'/>
        <% Genre.all.each do |genre| %>
      <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%= genre.name %></input>
    <% end %>
    </br>
</form>
