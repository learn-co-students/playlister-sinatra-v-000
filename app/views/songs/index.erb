<h2>Songs</h2>
<ul>
  <% @songs.each do |song| %>
    <li>
      <a href="/songs/<%= song.slug %>"><%= song.name %></a><br>
      Artist: <%= song.artist.name %><br>
      Genres: <% song.genres.each do |genre| %>
        <%= genre.name %>,
      <% end %>
    </li><br>
  <% end %>
</ul>
