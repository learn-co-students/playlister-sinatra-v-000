<h2>Index of all Songs</h2>
<% unless locals.empty? %>
  <%= message %>
<% end %>
<% Song.all.each do |song| %>
  <h3><a href='/songs/<%= song.slug %>'><%= song.name %></a></h3>
  <h5><%= song.artist.name if !!song.artist %></h5>
  <% song.genres.each do |genre| %>
    <h5><%= genre.name %></h5>
  <% end %>
<% end %>
