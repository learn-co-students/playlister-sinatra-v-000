<html>
<head>
  <title></title>
</head>
<body>
  <h1><%= @artist.name %></h1><br>
  <h2>Songs</h2>
  <ol>
    <% @artist.songs.each do |song| %>
    <li>
      <a href="/songs/<%= song.slug %>"><%= song.name %></a><br>
    </li>
    <% end %>
  </ol>  
  <h3>Genres</h3>   
  <ol>
    <% @artist.genres.each do |genre| %>
    <a href="/genres/<%= genre.slug %>"><li><%= genre.name %></li></a><br>
  </ol>
    <% end %>


</body>
</html>