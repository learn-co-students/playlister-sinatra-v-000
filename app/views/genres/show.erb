<h2><%= @genre.name %></h2>

<h3>Songs:</h3>
<% @genre.songs.each do |song| %>
  <ul><a href="/songs/<%= song.slug %>"><%= song.name %></a></ul>
    <% end %>

  <h3>Artists:</h3>
    <% @genre.artists.each do |artist| %>
      <ul><a href="/artists/<%= artist.slug %>"><%= artist.name%></a></ul>
      <% end %>
