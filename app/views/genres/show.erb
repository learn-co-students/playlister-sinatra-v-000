<h1><%= @genre.name %></h1>
<ol> <%= @genre.name %>: Songs:
<% @genre.songs.each do |song| %>
  <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
<% end %>
</ol>
<ol><%= @genre.name %>: Artists:
  <% @genre.artists.each do |artist| %>
    <li><a href='/artists/<%= artist.slug %>'><%= artist.name %></li>
  <% end %>
</ol>
