<h1>Genre: <%= @genre.name %></h1>
<ul>
  <li>
    <h3>Artists:</h3>
    <ul>
      <% @artists.each do |artist| %>
        <li>
          <a href="/artists/<%= artist.slug %>"><%= artist.name %></a>
        </li>
        
      <% end %>
    </ul>
  </li>
  <li>
    <h3>Songs:</h3>
    <ul>
      <% @songs.each do |song| %>
        <li>
          <a href="/songs/<%= song.slug %>"><%= song.name %></a>
        </li>
        
      <% end %>
    </ul>
  </li>
</ul>
