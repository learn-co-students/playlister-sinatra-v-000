<h1>Genre Name: <%= @genre.name %> </h1>
<br>

<h2>Genres Artists:</h2>
<% @genre.artists.each do |artist| %>
  <h3><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></h3>
<% end %>

<br>

<h2>Genres Songs:</h2>
<% @genre.songs.each do |song| %>
  <h3><a href="/songs/<%= song.slug %>"><%= song.name %></a></h3>
<% end %>
