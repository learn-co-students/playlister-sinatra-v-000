<h4><a href="/artists">Artists</a> - <a href="/songs">Songs</a> - <a href="/genres">Genres</a></h4>
<h2><u><%= @genre.name %></u></h2>
<h3><label>Artists:</label></h3>
  <ul>
    <% @genre.artists.each do |artist| %>
      <li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li>
    <% end %>
  </ul>
  <h3><label>Songs:</label></h3>
  <ul>
    <% @genre.songs.each do |song| %>
      <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
    <% end %>
  </ul>
