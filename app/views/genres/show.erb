<h1>Here's Your Genre:</h1>

<h2>Genre: <%=@genre.name%></h2>

<h2>Songs: </h2>

  <%@songs.uniq!%>

  <%@songs.each do |song|%>

    <p><a href="/songs/<%=song.slug%>"><%=song.name%></a></p>
  <%end%>

  <h2>Artists: </h2>
  
  <%@artists.uniq!%>
  <%@artists.each do |artist| %>

    <p><a href='/artists/<%=artist.slug%>'><%=artist.name%></a></p>
  <%end%>
  




 