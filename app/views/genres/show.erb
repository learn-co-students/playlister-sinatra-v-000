<h1><%= @genre.name %></h1>

<h2>Songs:</h2>
<ul>
<% @genre.songs.each { |song| %>
	<li>
		<a href="/songs/<%= song.slug %>"><%= song.name%></a>
	</li>
<% } %>
</ul>

<h2>Artists:</h2>
<ul>
<% @genre.artists.each { |artist| %>
	<li>
		<a href="/artists/<%= artist.slug %>"><%= artist.name%></a>
	</li>
<% } %>
</ul>