<h1><%= @genre.name %> </h1>
<br>

<h2>Songs:</h2>
  <ul> <% @genre.songs.each do |song| %>
    <li><a href="/songs/<%= song.slug %>"><li><%= song.name %></a></li>
    <% end %>
  </ul>

<h2>Artists:</h2>
  <ul> <% @genre.artists.each do |artist| %>
    <li><a href="/artists/<%= artist.slug %>"><li><%= artist.name %></a></li>
    <% end %>
  </ul>
