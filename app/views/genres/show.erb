<h1><%= @genre.name %></h1>
<h2>
  Songs:
  <% @genre.songs.each do |song| %>
    <a href="/songs/<%= song.slug %>"><%= song.name %></a>
  <% end %>
</h2>
<h3>
  Artists:
  <% @genre.artists.each do |artist| %>
    <a href="/artists/<%= artist.slug %>"><%= artist.name %></a>
  <% end %>
</h3>
<form action=<%="/genres/#{@genre.slug}/delete"%> method="POST">
	<input type="submit" value="delete">
</form>
<br>
<form action=<%="/genres/#{@genre.slug}/edit"%> method="GET">
	<input type="submit" value="edit">
</form>
