<h1><strong style="color: green">Genre Name:</strong> <%= @genre.name %></h1>
<h2><strong style="color: blue">Artists:</strong></h2>
<% @genre.artists.all.each do |artist| %>
  <p><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></p>
<% end %>
<h2><strong style="color: red">Songs:</strong></h2>
<% @genre.songs.all.each do |song| %>
  <p><a href="/songs/<%= song.slug %>"><%= song.name %></a></p>
<% end %>
