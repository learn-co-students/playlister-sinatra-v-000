<h1><%= @genre.name %></h1>
<% @songs.each do |song| %>
  <h3><a href="/songs/<%= song.slug %>"><%= song.name %></a></h3>
<% end %>
<% @artists.each do |artist| %>
  <h3><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></h3>
<% end %>
<form action="/genres/<%= @genre.slug %>/delete" method="post">
  <input type="hidden" name="_method" value="delete" id="hidden">
  <input type="submit" value="delete">
</form>
