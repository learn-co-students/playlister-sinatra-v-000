<h1>Genre Name: <%= @genre.name %> </h1>
<h2>Songs:</h2>
<% @genre.songs.each do |song|%>
  <ul><li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li></ul>
<% end %>

<h2>Artists:</h2>
<% @genre.artists.each do |artist|%>
  <ul><li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li></ul>
<% end %>
