<h1> <%= @genre.name %> </h1>
 <h2>Songs: <% @genre.songs.each do |song| %>
  <li> <a href="/songs/<%= song.slug %>"> <%= song.name %> </a> </li>
 <% end %></h2>

 <h2>Artists: <% @genre.artists.each do |artist| %>
   <li> <a href="/artists/<%= artist.slug %>"> <%= artist.name %> </a> </li>
<% end %></h2>
