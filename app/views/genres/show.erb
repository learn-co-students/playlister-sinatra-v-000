<h4>Genre Name:</h4>
<h2><%=@genre.name%></h2><br>
<hr>

<h4>Genres Artists:</h4>
<ul>
<% @genre.artists.each do |artist| %>
   <li><a href="/artists/<%=artist.slug%>"</a><%=artist.name%></a></li><br>
<%end %>
</ul>
<hr>
<h4>Genres Songs:</h4>
<ul>
<% @genre.songs.each do |song| %>
   <li><a href="/songs/<%=song.slug%>"</a><%=song.name%></a></li><br>
<%end %>
</ul>
