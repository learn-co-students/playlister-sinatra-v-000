<h2><%=@genre.name%></h2>
  <h3><strong>Song(s)</strong></h3>
  <%@genre.songs.map do |song| %>
    <a href="/songs/<%=song.slug%>"><%=song.name%></a><%=" - #{song.artist.name}"%>
    <br><br>
  <%end%>

  <h3><strong>Artist(s)</strong></h3>
    <% @genre.artists.order('name ASC').uniq.each_with_index do |artist, index| %>
        <a href="/artists/<%=artist.slug%>"><%="#{artist.name}"%></a>
        <br><br>
    <%end%>
