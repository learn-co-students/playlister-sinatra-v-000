<label>Genre:</label>
<h1><%= @genre.name %></h1>

<label><h3>Artists belonging to <%= @genre.name %> genre:</h3></label>
<% @genre.artists.each { |artist| %>
 <a href="/artists/<%= artist.slug %>"><%= artist.name %></a><br>
<% } %>
<br></br>
<label><h3>Songs belonging to <%= @genre.name %> genre:</h3></label>
<% @genre.songs.each { |song| %>
 <a href="/songs/<%= song.slug %>"><%= song.name %></a><br>
<% } %>
