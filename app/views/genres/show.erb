<h3><a href="/genres">All Genres</a></h3>
<h1>Genre: <%=@genre.name%></h1>
<h2>Songs:</h2>
<ul>
  <% @genre.songs.uniq.each do |song| %>
  	<li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
  <% end %>
</ul>
<h2>Artists:</h2>
<ul>
  <% @genre.artists.uniq.each do |artist| %>
    <li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li>
  <% end %>
</ul>