<h1><%=@genre.name%></h1>
<br></br>
<h2>Songs:</h2>
<% @genre.songs.each do |song| %>
  <ul>
    <li><%=song.name%><a href="/songs/<%=song.slug%>"><%=song.name.capitalize%>'s Page</a></li>
    <br></br>
  </ul>
<% end %>
<h2>Artists:</h2>
<% @genre.artists.each do |artist| %>
  <ul>
    <li><%=artist.name%><a href="/artists/<%=artist.slug%>"><%=artist.name.capitalize%> Page</a></li>
    <br></br>
  </ul>
<% end %>
