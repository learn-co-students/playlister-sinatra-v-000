<h1><%= @genre.name %></h1>

<h2>Artists: <ul><% @genre.artists.each do |artist| %>
      <li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li>
    <% end %></ul></h2>

<h2>Songs: <ul><% @genre.songs.each do |song| %>
      <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
    <% end %></ul></h2>
