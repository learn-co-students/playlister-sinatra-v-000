
<h1><%= @genre.name %></h1>
<% @genre.artists.each do |artist| %>
<p>Artist: <a href="/artists/<%= artist.slug %>"><%= artist.name %></a><br>
    <% artist.songs.each do |song| %>
        <% if SongGenre.find_by(song_id: song.id).genre_id == @genre.id %>
            Songs: <a href="/songs/<%= song.slug %>"><%= song.name %></a></p>
        <% end %>
    <% end %>
<% end %>
