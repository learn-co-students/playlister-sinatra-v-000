<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1><%= @genre.name %></h1>

    <h2>Artists</h2>
    <% @artists.each do |artist| %>
      <a href="/artists/<%= artist.slug %>"><p><%= artist.name %></p></a>
    <% end %>

    <h2>Songs</h2>
    <% @songs.each do |song| %>
      <a href="/songs/<%= song.slug %>"><p><%= song.name %></p></a>
    <% end %>
  </body>
</html>
