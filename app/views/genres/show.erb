<h1>Genre: <%=@genre.name%></h1>

<h2>Artists:</h2>
<% @genre.artists.each do |artist| %>
<h2><a href="/artists/<%=artist.slug%>"><%=artist.name%></a></h2>
<% #binding.pry %>
<% end %>

<h2>Songs:</h2>
<% @genre.songs.each do |song| %>
<h2><a href="/songs/<%=song.slug%>"><%=song.name%></a></h2>
<% end %>