<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h2><%= @genre.name %></h2>
    <h3>Songs</h3>
    <% @genre.songs.each do |song| %>
    <li><a href="/songs/<%= song.slug %>">
    <%= song.name %>
    </a> 
    </li>
    <% end %>
    <h3>Artists</h3>
    <% @genre.artists.each do |artist| %>
    <li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li>
    <% end %>
  </body>
</html>
