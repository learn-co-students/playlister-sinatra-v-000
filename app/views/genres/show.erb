<h3><%=@genre.name%></h3>

<h3>Songs</h3>
  <ul>
    <%@genre.songs.each do |song|%>
      <li><a href = "/songs/<%=song.slug%>"><%=song.name%></li></a>
    <%end%>
  </ul>

<h3>Artists</h3>
  <ul>
    <%@genre.artists.each do |artist|%>
      <li><a href = "/artists/<%=artist.slug%>"><%=artist.name%></li></a>
    <%end%>
  </ul>