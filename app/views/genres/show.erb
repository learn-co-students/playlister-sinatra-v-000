<h1>Genre name: <%= @genre.name %></h1>
<h2>Artists: </h2>
<ul>
  <% @genre.artists.each do |artist| %>
    <li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a>
      <ul>
        <% artist.songs.each do |song| %>
          <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>
