<h1><%=@genre.name%></h1>

<h4>Artists:</h4>
<ul>
  <%@genre.artists.each do |artist|%>
    <a href="/artists/<%= artist.slug %>"> <%= artist.name %><br>
  <%end%>
</ul>

<h4>Songs:</h4>
<ul>
  <%@genre.songs.each do |song|%>
    <a href="/songs/<%= song.slug %>"> <%= song.name %><br>
  <%end%>
</ul>