<h3> All of <%= @genre.name %>'s Artists</h3>
  <ul>
    <% @genre.artists.each do |artist| %>
      <li>
        <a href='/artists/<%= artist.slug %>'>
          <%= artist.name %>
        </a>
      </li>
    <% end %>
  </ul>

<h3>All of <%= @genre.name %>'s Songs:</h3>
  <ul>
    <% @genre.songs.each do |song| %>
      <li>
        <a href='/songs/<%= song.slug %>'>
          <%= song.name %>
        </a>
      </li>
    <% end %>
  </ul>
