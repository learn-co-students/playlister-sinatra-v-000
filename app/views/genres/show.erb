<h1><%= @genre.name %></h1>

<h3>Artists:</h3>

<ul>
 <% @genre.songs.each do |song| %>
    <li> <%= song.artist.name %> </li>
    <p> <a href="/artists/<%= song.artist.slug %>"></a></p>
  <%end%>
</ul>

<h3>Songs:</h3>
<ul>
 <% @genre.songs.each do |song| %>
    <li> <%= song.name %> </li>
    <p> <a href="/songs/<%= song.slug %>"></a></p>
  <%end%>
</ul>
