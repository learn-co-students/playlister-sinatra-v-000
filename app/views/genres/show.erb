<h1><%= @slug.name %></h1>

<h2>Artists</h2>
<ul>
  <% @slug.artists.each do |a| %>
  <li><a href="/artists/<%=a.slug%>"></a>"><h3><%= a.name %></h3></a></li>
  <% end %>
</ul>

<h2>Songs</h2>
<ul>
  <% @slug.songs.each do |s| %>
  <li><a href="/songs/<%=s.slug%>"></a>"><h3><%= s.name %></h3></a></li>
  <% end %>
</ul>