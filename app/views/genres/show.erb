<h2><%= @genre.name %><h2>

<h3>Songs:</h3>
  <% @genre.songs.each.do |song| %>
    <h4><a href="/songs/<%= song.slug %>"><%= song.name %></a></h4>
  <% end %>
<h3>Artists:</h3>
  <% @genre.artists.each.do |artist| %>
    <h4><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></h4>
  <% end %>
