<h3>Your favorite songs and artists in:</h3><h1><%=@genre.name%></h1>

<div class="genre-listings">
  <ol>
    <%@genre.songs.each do |song|%>
      <li><a href="/songs/<%=song.slug%>"><%=song.name%></a> by: <a href="/artists/<%=song.artist.slug%>"><%=song.artist.name%></a></li>
    <%end%>
  </ol>
</div>
