<h1>Genre "<%= @genre.name.capitalize %>" Page</h1>
<h2>Songs:
  <% @my_songs.each do |song| %>
  <a href="/songs/<%= song.slug %>"><%= song.name %></a><br>
  <% end %>
</h2>
<h2>Artists:
  <% @my_songs.each do |song| %>
      <a href="/artists/<%= song.artist.slug %>"><%= song.artist.name %></a><br>
  <% end %>
</h2>
