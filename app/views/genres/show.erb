<h2><%=@genre.name%></h2>

<h3>Songs</h3>
<ul>
  <%artists = []%>
<%@genre.songs.each do |song|%>
  <li><a href='/songs/<%=song.slug%>'><%=song.name%></a></li>
  <%artists << song.artist%>
  <br></br>
<%end %>
</ul>

<h3>Artists</h3>
<ul>
  <%artists.uniq%>
  <%artists.each do |artist|%>
    <li><a href='/artists/<%=artist.slug%>'><%=artist.name%></a></li>
    <br></br>
  <%end%>
</ul>
