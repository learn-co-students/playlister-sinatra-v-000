<h1><%= @genre.name %></h1>
<h2>Artists:</h2>
<% @genre.artists.uniq.each do |artist| %>
	<p><a href="/artists/<%=artist.slug%>"><%=artist.name%></a></p>
<% end %>
<h2>Songs:</h2>
<% @genre.songs.uniq.each do |song| %>
	<p><a href="/songs/<%=song.slug%>"><%=song.name%></a></p>
<% end %>
<p></p>
<h3><a href="/genres">Genre Index</a></h3>