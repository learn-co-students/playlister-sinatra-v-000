<h1>Genre Details</h1>
<h2><%= @genre.name %></h2>

<ul><p>Artists: <% @genre.artists.each do |artist| %>
<li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li>
<% end %></ul>
</p>

<ul><p>Songs: <% @genre.songs.each do |song| %>
<li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
<% end %></ul></p>
