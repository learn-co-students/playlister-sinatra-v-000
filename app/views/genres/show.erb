<h4>Genre: <%=@genre.name%></h4>

<h4>Song(s):
  <ul>
  <%@genre.songs.each do |song|%>
    <li><a href="/songs/<%=song.slug%>"><%=song.name%></a> </li>
  <%end%>
 </ul>

<h4>Artist(s):</h4>
<ul>
      <%@genre.artists.each do |artist|%>
      <li><p><a href='/artists/<%=artist.slug%>'><%=artist.name%></a></p></li>
      <%end%>
  </ul>
