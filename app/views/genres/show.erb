<h1>Genre Details</h1>
<p>Name: <%=@genre.name%></p>
<p>Songs: <%@genre.songs.each do |song|%></p>
  <p><a href="/songs/<%= song.slug%>"><%= song.name%></a></p>
  <%end%>

<p>Artists: <%@genre.artists.each do |artist|%></p>
  <p><a href="/artists/<%= artist.slug%>"><%= artist.name%></a></p>
  <%end%>