
<h1> <%= @genre.name %></h1>
<h3>Songs</h3>
  <ul>
      <% @genre.songs.each do |s| %>
        <li><a href="/songs/<%= s.slug%>"><%= s.name %> </a></li>
      <% end %>
  </ul>

<h3>Artists </h3>
  <ul>
      <% @genre.artists.each do |s| %>
        <li><a href="/artists/<%= s.slug%>"><%= s.name %> </a></li>
      <% end %>
  </ul>
