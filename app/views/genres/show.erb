<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1><%=@genre.name%></h1>
    by <%@genre.songs.each do |song|%>
    <p><%=song.name%></p>
    <a href="/songs/<%=song.slug%>"><%=song.name%></a>
    <%end%>

    <%@genre.songs.each do |song|%>
      <p>Artists:<%=song.artist.name%></p>
      <a href="/artists/<%=song.artist.slug%>"><%=song.artist.name%></a>
    <%end%>
  </body>
</html>
