<h2><%= @genre.name%></h2>
<h3>Songs:</h3>
<ul>
  <% @songs.each do |song| %>
    <h5><li><a href="/songs/<%= song.slug %>"><%= song.name %> </a></li></h5>
  <% end %>
</ul>

<h3>Artists:</h3>
<ul>
  <% @artists.each do |artist| %>
    <h5><li><a href="/artists/<%= artist.slug %>"><%= artist.name %> </a></li></h5>
  <% end %>
</ul>