<h1> <%= @genre.name%> show page</h1>

<h2> Genre: <%= @genre.name %> </h2>
<h2> <%= @genre.name %> Songs:</h2>
  <% @genre.songs.each do |song| %>
    <li><a href= '/songs/<%= song.slug %>'> <%= song.name %> </a></li>
  <% end %>

<h2><%= @genre.name %> Artists:</h2>

  <% @genre.artists.each do |artist| %>
    <ul><a href= '/artists/<%= artist.slug %>'> <%= artist.name %> </a></ul>
  <% end %>
