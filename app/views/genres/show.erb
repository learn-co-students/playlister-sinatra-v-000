<h1><%=@genre.name%></h1>

<% @genre.songs.each do |song| %>
 <strong>Song:</strong> <%= song.name%>
 <p>View more about <a href="/songs/<%=song.slug%>"><%= song.name%></a></p>
<% end %>

<% @genre.artists.each do |artist| %>
 <strong>Artist:</strong> <%= artist.name%>
 <p>View more by <a href="/artists/<%=artist.slug%>"><%= artist.name%></a></p>
<% end %>
