<h1><%=@genre.name%></h1>
<h2>Genre Songs</h2>
<ul>
  <%@genre.songs.each do |song|%>
    <li>
      <a href="/songs/<%=song.slug%>"><%=song.name%>
    </li>
    <%end%>
</ul>
<h2>Genre Artists</h2>
<ul>
  <%@genre.artists.each do |artist|%>
    <li>
      <a href="/artists/<%=artist.slug%>"><%=artist.name%>
    </li>
    <%end%>

</ul>
