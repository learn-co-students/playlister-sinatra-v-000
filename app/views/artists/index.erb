<%@artists.each do |artist|%>
<h2><a href="/artists/<%=artist.slug%>"><%=artist.name%></a></h2>
<h3>Genres:</h3>
<ul>
<%artist.genres.each do |genre|%>
  <li><%=genre.name%></li>
<%end%>
</ul>

<h3>Songs:</h3>
<ul>
<%artist.songs.each do |song|%>
  <li><%=song.name%></li>
<%end%>
</ul>

<hr>
<%end%>
