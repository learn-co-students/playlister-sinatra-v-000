<h1><%= @artist.name %></h1>

<p>
  <b>Genres: </b>
    <ol>
      <% @artist.genres.each do |genre| %>
        <li>
          <%= genre.name %> <a href="/genres/<%= genre.slug %>">View</a>
        </li>
      <% end %>
    </ol>
</p>
<p>
  <b>Songs: </b>
    <ol>
      <% @artist.songs.each do |song| %>
        <li>
          <%= song.name %> <a href="/songs/<%= song.slug %>">View</a>
        </li>
      <% end %>
    </ol>
</p>
