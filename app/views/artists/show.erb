<h1> <%= @artist.name%> show page</h1>

<h2> Artist Name: <%= @artist.name %> </h2>
<h2> Songs:</h2>
  <% @artist.songs.each do |song| %>
    <li><a href= '/songs/<%= song.slug %>'> <%= song.name %> </a></li>
  <% end %>

<h2>Genre(s):</h2>

  <% @artist.genres.each do |genre| %>
    <ul><a href= '/genres/<%= genre.slug %>'> <%= genre.name %> </a></ul>
  <% end %>
