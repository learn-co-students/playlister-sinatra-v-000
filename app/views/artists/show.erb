<h2><%= @artist.name %></h2>

<h3>Genres:

<% @artist.genres.each do |genre| %>
  <a href="/genres/<%= genre.slug %>"</a><%= genre.name.capitalize %></a>
<% end %>
</h3>


<h3>Songs</h3>

<ul>
<% @artist.songs.each do |song| %>
  <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
<% end %>
</ul>
