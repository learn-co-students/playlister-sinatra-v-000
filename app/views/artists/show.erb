<h1> <%=@artist.name%> </h1>

<br>

<h2>Songs</h2>
<%@artist.songs.each do |song|%>
  <p><h3><a href="/songs/<%= song.slug %>"><%= song.name %> </a></h3></p>
<%end%>

<br>

<h2>Genres</h2>
<%@artist.genres.each do |genre|%>
  <p><h3><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></h3></p>
<%end%>
