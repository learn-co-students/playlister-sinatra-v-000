<h1>Name: <%=@artist.name%></h1>
<h2>Songs:</h2>
 <ul>
  <%@artist.songs.each do |song|%>
    <li><%=song.name%></li>
    <li><a href='/songs/<%=song.slug%>'>Song Page</a></li>
  <%end%>
<h2>Genres:</h2>
  <%@artist.genres.each do |genre|%>
    <li><%=genre.name%></li>
    <li><a href='/genres/<%=genre.slug%>'>Genre Page</a>
    <%end%>
 </ul>
