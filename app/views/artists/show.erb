<h1><%= @artist.name %></h1>
<h3><%= @artist.name %> Songs:</h3>
  <% @artist.songs.each do |song| %>
    <a href="/songs/<%= song.slug %>"><%= song.name %></a></h3>

  <% end %>
  <br>

<h3>Listed in the following Genres:</h3>
  <% @artist.genres.each do |genre| %>
    <a href="/genres/<%= genre.slug %>"><h3><%= genre.name %></h3></a>

  <% end %>
