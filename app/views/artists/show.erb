<h1><%= @artist.name %></h1>
<br>
<h2> Song List: </h2>
  <% @artist.songs.each do |song| %>
   <p><a href="/songs/<%=song.slug%>"><%= song.name %></a></p>
  <%end%>
  <br>
  <p>Genre tags: <% @artist.genres.each do |genre| %>
    <a href="/genres/<%=genre.slug%>"><%= genre.name%></a>
    <%end%>
  </p>
