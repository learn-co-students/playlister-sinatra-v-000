<h1> Welcome to the Artist Page </h1>

<h2>Artist Name: <%= @artist.name %></h2>

<h3>Artist Songs:</h3>

<% @artist.songs.each do |song| %>
  <h4><a href="/songs/<%=song.slug%>"> <%=song.name%> </a></h4>
<% end %>

<br></br>

<h3>Artist Genres:</h3>

<% @artist.genres.each do |genre| %>
<h4><a href="/genres/<%=genre.slug%>"> <%=genre.name%> </a></h4>
<% end %>
