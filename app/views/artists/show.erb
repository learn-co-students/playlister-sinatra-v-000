<h1><%=@artist.name%></h1>

<h2>Songs:</h2>
<%@artist.songs.each do |song_obj|%>
<h3><a href="/songs/<%=song_obj.slug%>"><%=song_obj.name%></a></h3>
<%end%>

<br></br>
<h2>Genres:</h2>
<%@artist.genres.each do |genre_obj|%>
<h3><a href="/genres/<%=genre_obj.slug%>"><%=genre_obj.name%></a></h3>
<%end%>
