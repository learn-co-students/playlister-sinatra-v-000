<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1><%= @artist.name %></h1>
    <h3>Artist Songs</h3>
    <% @artist.songs.each do |song| %>
      <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
    <% end %>
    <br><br>
    <h3>Artist Genres</h3>
    <% @artist.genres.each do |genre| %>
      <li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li>
    <% end %>
    <br>
    <a href="/">Back to Home</a>
  </body>
</html>
