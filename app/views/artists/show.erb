
<h1><%=@artist.name%></h1>

<h3>Songs:</h3>
    <ul>
        <%@artist.songs.each do |song|%>
        <li><p><a href='/songs/<%=song.slug%>'><%=song.name%></a><p></li>
        <%end%>
    </ul>

<h3>Genres:</h3>
    <ul>
        <%@artist.genres.each do |genre|%>
        <li><p><a href='/genres/<%=genre.slug%>'><%=genre.name%></a></p></li>
        <%end%>
    </ul>
