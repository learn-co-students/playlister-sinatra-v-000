<h4><a href="/artists">Artists</a> - <a href="/songs">Songs</a> - <a href="/genres">Genres</a></h4>
<h2><u><%= @artist.name %></u><h2>
  <h3><label>Songs:</label></h3>
    <ul>
      <% @artist.songs.each do |song| %>
        <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
      <% end %>
    </ul>
  <h3><label>Genres:</label></h3>
    <ul>
      <% @artist.genres.each do |genre| %>
        <li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li>
      <% end %>
    </ul>
