	
<h1><%= @artist.name %></h1>
 <h3>Songs:
<ul>
  <% @artist.songs.each do |song| %>
    <li>
      <a href="/songs/<%= song.slug %>"><%= song.name %>
      </a>
    </li>
  <% end %>
</ul>
</h3>
 <h3>Genres:
<ul>
  <% @artist.genres.each do |genre| %>
    <li>
      <a href="/genres/<%= genre.slug %>"><%= genre.name %>
      </a>
    </li>
  <% end %>
</ul>
</h3>
 <p><a href="/">Homepage</a></p>
 <p><a href="/artists">Back to artist index</a></p>