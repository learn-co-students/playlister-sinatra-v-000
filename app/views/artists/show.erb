<h1><%= @artist.name %> </h1>
<br>

<h2>Songs:</h2>
  <ul> <% @artist.songs.each do |song| %>
    <li><a href="/songs/<%= song.slug %>"><li><%= song.name %></a></li>
    <% end %>
  </ul>

<h2>Genres:</h2>
  <ul> <% @artist.genres.each do |genre| %>
    <li><a href="/genres/<%= genre.slug %>"><li><%= genre.name %></a></li>
    <% end %>
  </ul>
