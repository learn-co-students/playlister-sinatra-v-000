<h2><%= @artist.name.capitalize %>'s Page </h2>

<h2>Song(s)</h2>
<% @artist.songs.each do |song| %>
  <p><a href="/songs/<%= song.slug %>"><%= song.name %></a></p>
<% end %>

<h2>Genre(s)</h2>
<% @artist.genres.each do |genre| %>
  
  <p><a href="/genres/<%= genre.slug %>"></a><%= genre.name %></p>
  
<% end %>