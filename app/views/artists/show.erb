<label>Artist Song(s):</label>
<br>
<% @artist.songs.each do |song| %>
<li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li> <br>
<% end %>

<label>Artist Genre(s):</label>
<br>
<% @artist.genres.each do |genre| %>
<li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li> <br>
<% end %>
