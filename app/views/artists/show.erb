<h2><%=@artist.name%></h2>

<%genres = []%>

  <h3>Songs</h3>
  <ul>
    <%@artist.songs.each do |song|%>
      <li><a href="/songs/<%=song.slug%>"><%=song.name%></a></li>
      <br></br>
        <%song.genres.each do |genre|%>
          <%genres << genre%>
        <%end%>
    <%end%>
  </ul>

  <h3>Genres</h3>
  <ul>
    <%genres.each do |genre|%>
      <li><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></li>
      <br></br>
    <%end%>
  </ul>
