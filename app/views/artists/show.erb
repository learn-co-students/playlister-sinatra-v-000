<a href="/songs/<%= @artist.slug %>"><h1>Artist: <%= @artist.name %></h1></a>

<p><ul><strong>Genres</strong></ul></p>
<% @artist.genres.each do |g| %>
<li><a href="/genres/<%= g.slug %>"><%= g.name %></a></li>
<% end %>

<p><ul><strong>Songs</strong></ul></p>
<% @artist.songs.each do |g| %>
<li><a href="/songs/<%= g.slug %>"><%= g.name %></a></li>
<% end %>
