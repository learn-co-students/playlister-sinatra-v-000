<h1>Artist name: <%= @artist.name %></h1>
<h2>Songs: </h2>
<ul>
  <% @songs.each do |song| %>
    <li>
      <a href="/songs/<%= song.slug %>"><%= song.name %></a>
      <ul>
        <% song.genres.each do |genre| %>
        <li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>
