<h1><%= @artist.name %></h1>

<h2>Songs:</h2>
<ul>
  <% @artist.songs.each do |song| %>
  <a href="/songs/<%=song.slug%>"><li><h2><%= song.name %></h2></li></a>
  <% end %>
</ul>

<h2>Genres:</h2>
<ul>
  <% @artist.genres.each do |genre| %>
  <a href="/genres/<%=genre.slug%>"><li><h2><%= genre.name %></h2></li></a>
  <% end %>
</ul>
