<h1><%= @artist.name %></h1>
<h3>Songs: </h3>
<ul>
  <% @artist.songs.each do |s| %>
    <a href="/songs/<%= s.slug %>"><li> <%= s.name %></li></a>
  <% end %>
  </ul>
<h3>Genres:</h3>  
<ul>
<% @artist.genres.each do |g| %>
  <a href="/genres/<%= g.slug %>"><li> <%= g.name %></li></a>
<% end %>
</ul>