<h3><a href="/artists">All Artists</a></h3>
<h1>Artist: <%= @artist.name %></h1>
<p><u>Songs:</u></p>
<ul>
  <% @artist.songs.uniq.each do |s| %>
    <li><a href="/songs/<%= s.slug %>"><%= s.name %></a></li>
  <% end %>
</ul>
<p><u>Genres:</u></p>
<ul>
  <% @artist.genres.uniq.each do |g| %>
    <li><a href="/genres/<%= g.slug %>"><%= g.name %></a></li>
  <% end %>
</ul>