<h2>Artist: <%= @artist.name %></h2>

<br></br>

<h3>Song(s): </h3>
<ul>
  <% @artist.songs.each do |song| %>
    <li><a href="/songs/<%= song.slug %>"> <%= song.name %> </a></li>
  <% end %>
</ul>
  
<h3>Genre(s): </h3>
<ul>
  <% @artist.genres.each do |genre| %>
    <li></li><a href="/genres/<%= genre.slug %>"> <%= genre.name %></a></li>
    <br>
  <% end %>
</ul>
