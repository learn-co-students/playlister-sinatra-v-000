<h1><%= @artist.name %></h1>
<br><br>
<label for="">Songs:</label>
<ul>
  <%@artist.songs.each do |song|%>
    <li><a href="/songs/<%= song.slug %>"></a><%= song.name %></li>
  <%end%>
</ul>
<label for="">Genres:</label>
<ul>
  <%@artist.genres.each do |genre|%>
    <li><a href="/genres/<%= genre.slug %>"></a><%= genre.name %></li>
  <%end%>
</ul>
