<h2><%=@artist.name%></h2>
<h4>Song Names:</h4>
<%@artist.songs.each do |song|%>
  <ul>
    <li><a href="/songs/<%=song.slug%>"><%=song.name%></a></li>
  </ul>
  <%end%>
  <h4>Genres:</h4>
<%@artist.genres.each do |genre|%>
  <ul>
    <li><a href="/genres/<%=genre.slug%>"><%=genre.name.titleize%></a></li>
  </ul>
  <%end%>
