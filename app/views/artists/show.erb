<h2><%= @artist.name %></h2>

<h3> Songs </h3>
<ul><% @artist.songs.each do |s| %>
  <li>
    <a href="/songs/<%= s.slug %>"><%= s.name %></a>
    <ul>
      <% s.genres.each do |g| %>
      <li><a href="/genres/<%= g.slug %>"><%= g.name%></a></li>
      <% end %>
    </ul>
  </li>
  <% end %>
</ul>
