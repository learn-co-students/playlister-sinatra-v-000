<h1>Artist Name: <%= @artist.name %> </h1>
<h2>Songs:</h2>
<% @artist.songs.each do |song|%>
  <ul><li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li></ul>
<% end %>

<h2>Genres:</h2>
<% @artist.genres.each do |genre|%>
  <ul><li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li></ul>
<% end %>
