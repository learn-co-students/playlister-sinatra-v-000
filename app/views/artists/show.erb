<!DOCTYPE html>
<%=@artist.name%>

<br>

<% Song.all.each do |song| %>
<% if song.artist_id == @artist.id %>
  <a href='/songs/<%=song.slug%>'> <%=song.name%>  </a> <br>
  <% end %>
<% end %>

<br>

<% @artist.genres.each do |genre|%>
<a href='/genres/<%=genre.slug%>'> <%=genre.name%><br>
<% end %>
