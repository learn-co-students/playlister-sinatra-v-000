<h2>Artist Name: <%= @artist.name %></h2>
<h3>Songs: </h3>
<ul>
    <% @artist.songs.each do |song| %>
        <h4><li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li></h4>
    <% end %>
</ul>
<h3>Genres: </h3>
<ul>
    <% @artist.genres.each do |genre| %>
        <h4><li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li></h4>
    <% end %>
</ul>