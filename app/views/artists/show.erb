<html>
<head>
  <title>Show</title>
</head>
 <body>

   <h1>Artist Profile</h1><%=@artist.name %>
  <p>
    <h2>Songs: </h2>
      <% @artist.songs.each do |song| %>
      <%=song.name%>
      <a href='/songs/<%=song.slug%>'>Profile</a>
      <%end%>
  </p>

  <p>
    <h2>Genres: </h2>
      <% @artist.genres.each do |genre|%>
      <%= genre.name%>
      <a href='/genres/<%=genre.slug%>'>Profile</a>
      <%end%>
  </p>
 </body>
</html>
