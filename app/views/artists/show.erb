<h1><%= @artist.name %></h1>
<h3>Songs: </h3>
<ul class="song-list">
<% @artist.songs.each do |song| %>
    <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
<% end %>
</ul>

<h3>Genres: <%= @artist.genres.collect { |g| '<a href="/genres/' + g.slug + '">' + g.name + '</a>' }.join(", ")%></h3>