<h1><%= @artist.name %></h1>

<h3>Songs</h3>
  <ul>
    <% @artist.songs.each do |song| %>
      <li><a href="/songs/<%= song.name %>"><%= song.name %></a></li>
    <% end %>
  </ul>

<h3>Genres</h3>
  <ul>
    <% @artist.genres.each do |genre| %>
      <li><a href="/genres/<%= genre.name %>"><%= genre.name.capitalize %></a></li>
    <% end %>
  </ul>
