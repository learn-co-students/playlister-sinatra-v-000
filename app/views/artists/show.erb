<h1><%= @artist.name %></h1>

<h2>Songs: <ul><% @artist.songs.each do |song| %>
      <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
    <% end %></ul></h2>

<h2>Genres: <ul><% @artist.genres.each do |genre| %>
      <li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li>
    <% end %></ul></h2>
