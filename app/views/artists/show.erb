<h1>Artist Name: <%=@artist.name%> </h1>

<h2>Songs:</h2>
<ul>
<%@artist.songs.each do |song|%>
  <h2><li><a href="/songs/<%=song.slug%>"><%=song.name%></a></li></h2>
<%end%>
</ul>

<h2>Genres:</h2>
<ul>
<%@artist.genres.each do |genre|%>
  <h2><li><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></li></h2>
<%end%>
</ul>
