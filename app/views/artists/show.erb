<h1>Artist:<a href="/artists/<%=@artist.slug%>"> <%= @artist.name %> </a></h1>

<h3>Song Catalog:</h3>
  <ul>
 <% @artist.songs.each do |song| %>
    <li><a href="/songs/<%=song.slug%>"><%=song.name%></a></li>
    <%end%>
  </ul>

  <h3>Genres: </h3>
  <% @artist.genres.each do |genre|%>
  <input type="checkbox" name="<%=genre.name%>" id="genre">
  <a href="/genres/<%=genre.slug%>"> <%= genre.name %> </a>
  <%end%>
