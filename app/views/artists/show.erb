<h1><%= @artist.name %></h1>

<ul>
  <% @songs.each do |song| %>
    <br>
    <li>
      <a href="/songs/<%= song.slug %>">Song Name: <%= song.name %></a>
    </li>

    <% song.genres.each do |genre| %>
      <li>
        <a href="/genres/<%= genre.slug %>">Genre Name: <%= genre.name %></a>
      </li>
    <% end %>
  <% end %>
</ul>
