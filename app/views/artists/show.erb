<h5> ~~ <a href= "/artists">Artists</a> ~~ <a href= "/songs">Songs ~~ <a href= "/genres">Genres</a> ~~ </h5>

<h3><%= @artist.name %></h3>
  <h4><label>Songs:</label></h4>
    <ul>
    <% @artist.songs.each do |song| %>
      <li><a href="/songs/<%= song.slug %>"><%= song.name %> </a></li>
    <% end %>
    </ul>

    <h4><label>Genres:</label></h4>
    <ul>
      <% @artist.genres.each do |genre| %>
        <li><a href="/genres/<%= genre.slug %>"><%= genre.name %> </a></li>
      <% end %>
    </ul>
