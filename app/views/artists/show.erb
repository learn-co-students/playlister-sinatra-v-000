<h3><%=@artist.name%></h3>

<h3>Songs</h3>
  <ul>
    <%@artist.songs.each do |song|%>
      <li><a href = "/songs/<%=song.slug%>"><%=song.name%></li></a>
    <%end%>
  </ul>

<h3>Genres</h3>
  <ul>
    <%@artist.genres.each do |genre|%>
      <li><a href = "/genres/<%=genre.slug%>"><%=genre.name%></li></a>
    <%end%>
  </ul>