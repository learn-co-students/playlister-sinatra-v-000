<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1><%= @artist.name %></h1>

    <h2>Songs</h2>
    <% @songs.each do |song| %>
      <a href="/songs/<%= song.slug %>"><p><%= song.name %></p></a>
    <% end %>

    <h2>Genres</h2>
    <% @genres.each do |genre| %>
      <a href="/genres/<%= genre.slug %>"><p><%= genre.name %></p></a>
    <% end %>
  </body>
</html>
