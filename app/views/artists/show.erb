<h3>All Songs by <%= @artist.name %>:</h3>

<ul>
  <% @artist.songs.each do |song| %>
    <li>
      <a href='/songs/<%= song.slug %>'>
        <%= song.name %>
      </a>
    </li>
  <% end %>
</ul>

<h3>All <%= @artist.name %>'s Genres:</h3>

<ul>
  <% @artist.genres.each do |genre| %>
    <li>
      <a href='/genres/<%= genre.slug %>'>
        <%= genre.name %>
      </a>
    </li>
  <% end %>
</ul>
