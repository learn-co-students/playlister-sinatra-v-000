<h1><%= @artist.name %></h1>

<h2>Songs:</h2>
<ul>
<% @artist.songs.each { |song| %>
	<li>
		<a href="/songs/<%= song.slug %>"><%= song.name%></a>
	</li>
<% } %>
</ul>

<h2>Genres:</h2>
<ul>
<% @artist.genres.each { |genre| %>
	<li>
		<a href="/genres/<%= genre.slug %>"><%= genre.name%></a>
	</li>
<% } %>
</ul>