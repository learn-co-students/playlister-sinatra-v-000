<h1>Artist</h1>
<h2>Name: <%= @artist.name %></h2>
<h2>Song:</h2>
<ul>
   <% @artist.songs.each do |song| %>
      <li><h3><a href="/songs/<%= song.slug %>"><%= song.name %></a></h3></li>
   <% end %>
</ul>
<h2>Genre:</h2>
<ul>
   <% @artist.genres.each do |genre| %>
      <li><h3><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></h3></li>
   <% end %>
</ul>
