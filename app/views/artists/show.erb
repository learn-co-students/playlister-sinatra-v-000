<ul>
  <li> <%= @artist.name  %></li>
  <% @artist.songs.each do |song| %>
    <%= song.name %>
    <a href="/songs/<%= song.slug %>">Link to Song</a>
  <% end %>
  <a href="/song/<%= @artist.slug %>"></a>
  <% @artist.genres.each do |genre| %>
    <%= genre.name %>
    <a href="/genres/<%= genre.slug %>"></a>
  <% end %>
</ul>