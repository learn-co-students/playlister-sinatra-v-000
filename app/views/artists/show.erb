<h3> <%= @artist.name %> </h3>
<br>

<h4>Songs: </h4>
<br>
  <% @artist.songs.each do |song| %>
    <li> <a href="/songs/<%= song.slug %>"> <%= song.name %> </a>
      &nbsp;
      Genre: 
      <% song.genres.each do |genre| %>
          <a href="/genres/<%= genre.slug %>"><%= genre.name %> </a>
          &nbsp;
      <% end %>
    </li>
    <br>
  <% end %>
