<h3><%= @artist.name %></h3>

<p><b>Songs</b>:</p>
    <% @artist.songs.each do |song| %>
      <p><a href="/songs/<%= song.slug %>"><%= song.name %></a> </p>
    <% end %>

<p><b>Genres:</b></p>
    <p><% @artist.genres.each do |genre| %>
      <a href="/genres/<%= genre.slug %>"><%= genre.name %> </a>
    <% end %></p>
