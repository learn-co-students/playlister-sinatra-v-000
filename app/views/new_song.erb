<html>
<head>
  <title></title>
</head>
<body>
  <h1>New Song</h1>
  <form action="/songs" method="POST">
    <label>Song Name:</label>
    <input type="text"  name="Name"><br>
    <label>Artist:</label>
    <input type="text"  name="Artist Name"><br>
    <label>Genres: </label>
    <% @genres.each do |genre| %>
    <input type="checkbox" name="genres" value="<%= genre.name %>" id="<%= genre.name %>"><%= genre.name %>
    <% end %><br>
    <input type="submit" value="Create">
  </form>
</body>
</html>